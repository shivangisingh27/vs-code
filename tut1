#include <iostream>
#include <cstdio>
using namespace std;

int main() {
    // Complete the code.
    int n;int b;int a;
    string num[10]={"one","two","three","four","five","six","seven","eight","nine"};
    if(n>1&&n<9)
    {
        for(int i=a;i<=b;i++)
        {
            cout<<num[i]<<num[i+1]<<endl;
            cin>>num[i];
            cin>>num[i+1];
        }
    }
    if(n>9)
    {
        if(n%2==0)
        {
            cout<<"even"<<endl;
            
        }
        else {
            cout<<"odd"<<endl;
        
        }
    }
    return 0;
} 
